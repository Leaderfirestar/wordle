{
	"branches": [
		{
			"name": "master"
		},
		{
			"name": "staging",
			"prerelease": "alpha"
		}
	],
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/git",
		[
			"@semantic-release/exec",
			{
				"prepareCmd": "mv dist.zip dist-v${nextRelease.version}.zip"
			}
		],
		[
			"@semantic-release/github",
			{
				"assets": "dist-v${nextRelease.version}.zip",
				"label": "Web Build"
			}
		]
	]
}
